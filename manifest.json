{
  "manifest_version": 2,

  "name": "WBR-ZP",
  "description": "Apply WBR race rules to zwiftpower results for uploading",
  "version": "1.0.1",

  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
	"http://www.zwiftpower.com/zwift.php?do=upload_results*",
	"https://www.zwiftpower.com/zwift.php?do=upload_results*"
  ]
}
